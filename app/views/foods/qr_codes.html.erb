<%= render partial: "qr_code", object: @all_qr_code %>

<hr>

<% @locations.each do |loc,qrcode| %>
  <%= render partial: "qr_code", object: qrcode %>
  <br>
<% end %>

<hr>

<% @foods.each do |food| %>
  <% url_for_qr_code = "#{@url_for_qr_code_base}/#{food.id}"  %>
  <% edit_qr_code = RQRCode::QRCode.new( "#{url_for_qr_code}/edit", :size => 6, :level => :h ) %>
  <%= render partial: "qr_code", object: edit_qr_code %>
  <br>
<% end %>

